# Your wallet's private key (required)
# Remove the '0x' prefix if present
PRIVATE_KEY=your_private_key_here

# Reservoir API key (optional but recommended)
# Get one at: https://reservoir.tools/
RESERVOIR_API_KEY=your_reservoir_api_key_here

# RPC URL (required)
# Use a provider like Alchemy (https://www.alchemy.com/) or Infura (https://infura.io/)
RPC_URL=your_rpc_url_here

# NFT Collection Parameters
# The contract address of the collection you want to mint from
COLLECTION_ADDRESS=0xYourCollectionAddress

# Number of NFTs to mint (must be a positive integer)
QUANTITY_TO_MINT=5

# Maximum ETH price you're willing to pay per token (in ETH)
MAX_ETH_PER_TOKEN=0.1 